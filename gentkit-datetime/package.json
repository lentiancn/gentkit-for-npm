{
  "name": "gentkit-datetime",
  "version": "1.0.0",
  "description": "Date and Time handling library.",
  "keywords": [
    "node",
    "npm",
    "yarn",
    "lib",
    "commonjs",
    "module",
    "gentkit",
    "date",
    "time",
    "datetime"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitee.com/gentkit/gentkit-for-npm"
  },
  "license": "MIT",
  "author": "田隆 (Len)",
  "type": "module",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build && npm run test"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/jest": "^30.0.0",
    "rollup": "^4.53.3",
    "ts-jest": "^29.4.5",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  }
}
