{
  "name": "gentkit-datetime-beta4",
  "version": "1.0.0",
  "description": "Date and Time handling library.",
  "keywords": [
    "node",
    "npm",
    "yarn",
    "lib",
    "commonjs",
    "module",
    "gentkit",
    "date",
    "time",
    "datetime"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitee.com/gentkit/gentkit-for-npm"
  },
  "license": "MIT",
  "author": "田隆 (Len)",
  "type": "module",
  "main": "index.cjs",
  "module": "index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./index.js"
      },
      "require": {
        "types": "./index.d.cts",
        "default": "./index.cjs"
      }
    }
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "vitest run",
    "test:coverage": "vitest --coverage",
    "prepublishOnly": "npm run build && npm run test"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.14",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  }
}
